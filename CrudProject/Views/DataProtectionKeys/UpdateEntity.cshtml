@model DataProtectionKey


<div class="row mt-2">
    <div class="col-md-3">
    </div>
    <div class="col-md-6">

        <form method="post" asp-action="UpdateEntity" enctype="multipart/form-data">
            <h1 class="text-center text-primary">DataProtectionKeys Update</h1>
            <input asp-for="Id" type="hidden" />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="FriendlyName" class="control-label"></label>
                <input asp-for="FriendlyName" class="form-control" />
                <span asp-validation-for="FriendlyName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Xml" class="control-label"></label>
                <input asp-for="Xml" class="form-control" />
                <span asp-validation-for="Xml" class="text-danger"></span>
            </div>
      
            <div class="form-group ">
                <input type="submit" placeholder="Update" class="btn btn-outline-primary mt-2 btn-block" />
            </div>
        </form>
    </div>
</div>